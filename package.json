{
  "name": "@activewidgets/vue",
  "version": "3.4.4",
  "description": "Datagrid component for vue.js",
  "author": "ActiveWidgets SARL",
  "license": "MIT",
  "type": "module",
  "main": "./dist/main.js",
  "sideEffects": false,
  "exports": {
    ".": "./dist/main.js",
    "./index.js": "./dist/index.js",
    "./index.css": "./dist/index.css",
    "./style-inject": "./dist/style-inject.js"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
