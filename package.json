{
  "name": "@activewidgets/vue",
  "version": "0.0.5",
  "description": "Datagrid component for vue.js",
  "author": "webmaster@activewidgets.com",
  "license": "MIT",
  "main": "./dist/ax-package.umd.js",
  "module": "./dist/ax-package.esm.js",
  "files": [
    "dist/ax-*.*",
    "**/package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "e2e": "npm run storybook:build && npm run e2e:local",
    "e2e:local": "wdio ./test/config/wdio.local.js",
    "build": "rollup -c",
    "storybook": "start-storybook -p 9001 -c storybook",
    "storybook:build": "npm run build && rimraf out/storybook && build-storybook -c storybook -o out/storybook",
    "test": "jest"
  },
  "dependencies": {
    "@activewidgets/frameworks": "https://cdn.activewidgets.com/packages/frameworks-0.0.5.tgz",
    "@activewidgets/grid": "https://cdn.activewidgets.com/packages/grid-0.0.5.tgz"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "@storybook/vue": "^5.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "@wdio/cli": "^5.0.0",
    "@wdio/jasmine-framework": "^5.0.0",
    "@wdio/local-runner": "^5.0.0",
    "@wdio/spec-reporter": "^5.0.0",
    "acorn": "^6.0.0",
    "babel-jest": "^24.0.0",
    "babel-loader": "^8.0.0",
    "babel-preset-vue": "^2.0.0",
    "chromedriver": "^2.45.0",
    "express": "^4.0.0",
    "jest": "^24.0.0",
    "postcss-import": "^12.0.0",
    "rimraf": "^2.0.0",
    "rollup": "^1.20.0",
    "rollup-plugin-babel": "^4.0.0",
    "rollup-plugin-postcss": "^2.0.0",
    "rollup-plugin-sourcemaps": "^0.4.0",
    "rollup-plugin-terser": "^5.0.0",
    "style-inject": "^0.3.0",
    "vue": "^2.0.0",
    "vue-loader": "^15.0.0",
    "vue-template-compiler": "^2.0.0",
    "wdio-chromedriver-service": "^5.0.0",
    "webdriverio": "^5.0.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/test/unit"
    ],
    "testMatch": [
      "**/*.js"
    ],
    "moduleNameMapper": {
      "\\.(css|png)$": "<rootDir>/test/mocks/files.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/activewidgets/vue.git"
  },
  "keywords": [
    "vue"
  ],
  "bugs": {
    "url": "https://github.com/activewidgets/vue/issues"
  },
  "homepage": "https://github.com/activewidgets/vue#readme"
}
