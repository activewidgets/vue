{
  "name": "@activewidgets/vue",
  "version": "3.1.0",
  "description": "Datagrid component for vue.js",
  "author": "ActiveWidgets SARL",
  "license": "MIT",
  "main": "./dist/umd/package.js",
  "module": "./dist/esm/package.js",
  "files": [
    "dist/*/*.*",
    "*/package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rimraf dist/* && rollup -c && node rewrite-map.js",
    "examples": "npm run build && vite build",
    "prepack": "npm run build && rimraf out/*",
    "preview": "npm run examples && vite preview --open /",
    "start": "vite --open /",
    "test": "npm run examples && jest"
  },
  "dependencies": {
    "@activewidgets/datagrid": "3.1.0",
    "@activewidgets/frameworks": "3.1.0"
  },
  "peerDependencies": {
    "vue": "^3"
  },
  "devDependencies": {
    "@activewidgets/examples": "0.0.12",
    "@activewidgets/options": "3.1.0",
    "@activewidgets/puppeteer": "0.0.6",
    "@babel/core": "^7",
    "@babel/preset-env": "^7",
    "@rollup/plugin-babel": "^5",
    "@rollup/plugin-node-resolve": "^13",
    "@testing-library/vue": "^6",
    "@vitejs/plugin-vue": "^2",
    "@vue/compiler-sfc": "^3",
    "rimraf": "^3",
    "rollup": "^2",
    "rollup-plugin-md": "^1",
    "rollup-plugin-postcss": "^4",
    "rollup-plugin-sourcemaps": "^0.6",
    "rollup-plugin-terser": "^7",
    "style-inject": "^0.3",
    "vite": "^2",
    "vue": "^3"
  },
  "jest": {
   "preset": "@activewidgets/puppeteer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/activewidgets/vue.git"
  },
  "keywords": [
    "vue",
    "datagrid",
    "datatable",
    "virtualized",
    "scrolling",
    "grid",
    "table",
    "virtual",
    "data-grid",
    "data-table",
    "activewidgets"
  ],
  "bugs": {
    "url": "https://github.com/activewidgets/vue/issues"
  },
  "homepage": "https://activewidgets.com/vue/data-grid/"
}
