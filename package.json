    "vue": "^3"
  },
  "devDependencies": {
    "@activewidgets/examples": "0.0.16",
    "@activewidgets/options": "3.4.4",
    "@activewidgets/puppeteer": "0.1.3",
    "@testing-library/vue": "^7",
    "@vitejs/plugin-vue": "^4",
    "rollup-plugin-md": "^1",
    "vite": "^4",
    "vitest": "latest",
    "vue": "^3"
  },
  "overrides": {
    "jest": "npm:vitest"
  },
  "repository": {
    "type": "git",
