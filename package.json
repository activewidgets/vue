{
  "name": "@activewidgets/vue",
  "version": "3.0.6",
  "description": "Datagrid component for vue.js",
  "author": "ActiveWidgets SARL",
  "license": "MIT",
  "main": "./dist/umd/package.js",
  "module": "./dist/esm/package.js",
  "files": [
    "dist/*/*.*",
    "*/package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rimraf dist/* && rollup -c && node rewrite-map.js",
    "output": "npm run build && rimraf out/* && rollup -c rollup.examples.js",
    "prepack": "npm run build && rimraf out/*",
    "serve": "sirv out --port 80 --single",
    "start": "rollup -w -c rollup.examples.js",
    "test": "npm run output && jest"
  },
  "dependencies": {
    "@activewidgets/datagrid": "3.0.6",
    "@activewidgets/frameworks": "3.0.6"
  },
  "peerDependencies": {
    "vue": "^3.0.0-rc.1"
  },
  "devDependencies": {
    "@activewidgets/examples": "0.0.10",
    "@activewidgets/options": "3.0.6",
    "@activewidgets/puppeteer": "0.0.4",
    "@activewidgets/testing": "0.0.3",
    "@babel/core": "^7",
    "@babel/preset-env": "^7",
    "@vue/compiler-sfc": "^3.0.0-rc.1",
    "@rollup/plugin-alias": "^3",
    "@rollup/plugin-babel": "^5",
    "@rollup/plugin-commonjs": "^14",
    "@rollup/plugin-json": "^4",
    "@rollup/plugin-node-resolve": "^8",
    "@rollup/plugin-replace": "^2",
    "open": "^7",
    "rimraf": "^3",
    "rollup": "^2",
    "rollup-plugin-copy": "^3",
    "rollup-plugin-img": "^1",
    "rollup-plugin-livereload": "^1",
    "rollup-plugin-md": "^0.0.7",
    "rollup-plugin-postcss": "^3",
    "rollup-plugin-sourcemaps": "^0.6",
    "rollup-plugin-terser": "^6",
    "rollup-plugin-vue": "^6.0.0-beta.9",
    "sirv-cli": "^1",
    "style-inject": "^0.3",
    "vue": "^3.0.0-rc.1"
  },
  "alias": {
    "@activewidgets/vue": "./"
  },
  "jest": {
    "projects": [
      {
        "displayName": "Visual",
        "preset": "@activewidgets/puppeteer"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/activewidgets/vue.git"
  },
  "keywords": [
    "vue",
    "datagrid",
    "datatable",
    "virtualized",
    "scrolling",
    "grid",
    "table",
    "virtual",
    "data-grid",
    "data-table",
    "activewidgets"
  ],
  "bugs": {
    "url": "https://github.com/activewidgets/vue/issues"
  },
  "homepage": "https://activewidgets.com/vue/data-grid/"
}
